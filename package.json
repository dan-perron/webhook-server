{
  "name": "webhook-server",
  "version": "0.0.0",
  "type": "module",
  "private": true,
  "dependencies": {
    "@aws-sdk/client-s3": "^3.577.0",
    "@google/generative-ai": "^0.2.1",
    "@slack/bolt": "^3.12.2",
    "@slack/webhook": "^5.0.3",
    "@types/node-cron": "^3.0.11",
    "axios": "^1.7.2",
    "cheerio": "^1.0.0-rc.12",
    "chokidar": "^3.5.3",
    "config": "^3.3.10",
    "cookie-parser": "~1.4.4",
    "cross-env": "^7.0.2",
    "dayjs": "^1.11.13",
    "express": "~4.17.3",
    "http-errors": "~1.6.3",
    "mongodb": "6",
    "morgan": "~1.9.1",
    "node-cron": "^3.0.3",
    "node-json-transform": "^1.1.2",
    "object-hash": "^2.0.3",
    "openai": "^3.2.1",
    "pug": "3.0.1",
    "typescript": "^5.3.3",
    "yahoo-fantasy": "^5.2.2"
  },
  "devDependencies": {
    "@eslint/create-config": "0.4.6",
    "@typescript-eslint/eslint-plugin": "^6.20.0",
    "@typescript-eslint/parser": "^6.20.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.4.0",
    "husky": "^9.1.7",
    "prettier": "^3.2.4"
  },
  "scripts": {
    "format:check": "prettier --check .",
    "format:write": "prettier --write .",
    "lint:check": "eslint .",
    "lint:fix": "eslint --fix .",
    "fix": "yarn lint:fix && yarn format:write",
    "build": "tsc -p .",
    "prestart": "yarn build",
    "start": "node built/bin/www.js",
    "deploy": "./deploy.sh",
    "prepare": "husky install"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn build && yarn lint:check && yarn format:check"
    }
  }
}
